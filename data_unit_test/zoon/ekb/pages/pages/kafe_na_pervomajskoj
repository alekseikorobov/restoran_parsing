<script>
	requirejs(['jquery', 'lib/sly'], function($, Sly) {
		var $node = $('#uid_popup_premium');

		window.za($.extend({ev_type: 'stat', ev_action: 'show'}, $node.data('za')));

		$node.find('.js-popup-close').on('click', function(e) {
			e.preventDefault();
			closePopup()
		});

		$('body').append($node);

		var $slider = $node.find('.js-slider-block');
		var $sliderItem = $slider.find('.js-slider-item');

		var options = {
			horizontal: true,
			itemNav: 'basic',
			smart: true,
			activateMiddle: false,
			mouseDragging: false,
			touchDragging: true,
			releaseSwing: true,
			startAt: 0,
			scrollBy: 0,
			speed: 800,
			swingSpeed: 0.1,
			elasticBounds: true,
			slidebyone: true,
			prevPage: $slider.find('.js-prev'),
			nextPage: $slider.find('.js-next')
		};

		var sly = new Sly($slider, options);

		sly.on('load moveEnd', function(e) {
			$node.trigger('scroll');
		});

		$(window).on('resize.popup_premium_timer', function(e) {
			slidesResize();
			sly.reload();
		});

		$node.find('.js-org-url').on('click', function (e) {
			window.za($.extend({ev_type: 'stat', ev_action: 'open'}, $(this).data('za')));
		});

		function slidesResize() {
			var sliderWidth = $slider.width();
			var $similarHeight = $slider.find('.js-similar-height');
			var max_height = 0;
			$sliderItem.css('max-width', sliderWidth);
			$similarHeight.each(function () {
				max_height = Math.max($(this).height(), max_height);
			});
			$similarHeight.height(max_height);
		}

		$(window).on('resize', function(e) {
			slidesResize();
			sly.reload();
		});

		var popupAfterCall = $('#uid_popup_after_call');
		function showPopup() {
			if (popupAfterCall.length > 0 && popupAfterCall.data('popup-shown') === 1) {
				return;
			}

			$node.css('bottom', '-100%');
			$node.show();
			slidesResize();
			sly.init();
			$node.animate({
				bottom: 0
			}, 250, function() {
				$node.trigger('scroll');
			});
			window.za($.extend({ev_type: 'stat', ev_action: 'display'}, $node.data('za')));
					}

		function closePopup() {
			$node.animate({
				bottom: '-100%'
			}, 250, function() {
				$node.hide();
				sly.destroy();
			});
			window.za($.extend({ev_type: 'stat', ev_action: 'close'}, $node.data('za')));
		}

					showPopup();
			});
</script>


	
<script>
	requirejs(['jquery', 'js2/ZATimer'], function($, zaTimer) {
		zaTimer.track($('.js-social-stat'), {ev_action: 'display_short', ev_category: 'vk'}, {interval: 0, visible_percent: 1});
		zaTimer.track($('.js-specialists-stat'), {ev_action: 'display_short', ev_category: 'prof'}, {interval: 0, visible_percent: 1});
		zaTimer.track($('.js-video-stat'), {ev_action: 'display_short', ev_category: 'video'}, {interval: 0, visible_percent: 1});
		zaTimer.track($('.js-usersphoto-stat'), {ev_action: 'display_short'}, {interval: 0, visible_percent: 1});
		zaTimer.track($('.js-service-offers'), {ev_action: 'display_short', ev_category: 'offer'}, {interval: 0, visible_percent: 1});
		zaTimer.track($('.js-stat-groups'), {ev_action: 'display_short'}, {interval: 0, visible_percent: 1});

		new VisibilityTracker(document.querySelector('.js-menu-list'), function () {
			window.za({ ev_action: 'display_short', ev_category: 'price' })
		}, { ratio: 0, duration: 0 }).track();

		$('.js-show-more-images, .js-stat-groups-more, .js-show-more-posts').on('click', function(e) {
			e.preventDefault();
			var zaParams = $(this).data('za');
			if (zaParams) {
				window.za($.extend({ev_action: 'show_more'}, zaParams));
			}
		})

	})
</script>
