<script>
(function(){
	const node = document.getElementById('u11311737');

	const getTooltipContent = function (mark) {
		if (!mark) return '–û—Ü–µ–Ω–∏—Ç–µ –æ—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—é';
		return `–í–∞—à–∞ –æ—Ü–µ–Ω–∫–∞ ‚Äî ${mark}`;
	};

	const tooltipActivator = node.querySelector('.js-tooltip-activator');
	const tooltip = new Tooltip(Tooltip.createNode(), tooltipActivator);
	const ratingStars = node.querySelector('.js-rating-stars');

	tooltip.setContent(getTooltipContent(null));
	tooltipActivator.addEventListener('mouseenter', () => tooltip.open());
	tooltipActivator.addEventListener('mouseleave', () => tooltip.close());

	ratingStars.addEventListener('change', event => {
		tooltip.setContent(getTooltipContent(event.target.value));
	});
})();
</script>

<script type="module">
	import { RatingStars } from '//zoon.ru/build/main/pages/organization.js?v=AXiuo+Dz1FI';

