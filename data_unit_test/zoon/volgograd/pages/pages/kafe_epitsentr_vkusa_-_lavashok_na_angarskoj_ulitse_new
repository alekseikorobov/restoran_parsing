<script>
zrequire(["lib/swiper/swiper.min"], function (Swiper) {
	const node = document.getElementById('u46338426');
	const list = node.querySelector('.js-slider-container');
	const total = Number('4');
	const limit = Number('20');
	let offset = limit;
	let loading = false;

	function loadOffers () {
		return zrequest.api('Organization.GetOffers', {
			id: '637cfb46c7adee4fd408ef6f',
			offset: offset,
			limit: limit,
		}).then(function (result) {
			if (!result) return;
			offset += limit;
			return result;
		});
	}

	function updateList (items) {
		list.insertAdjacentHTML('beforeend', items.join(''))
	}

	const swiper = new Swiper(node.querySelector('.js-slider-block'), {
		cssWidthAndHeight: true,
		slidesPerView: 'auto',
		slidesPerGroupAuto: true,
		//spaceBetween: 20,
		visibilityFullFit: true,
		autoResize: true,
		slideActiveClass: 'active-slide',
		slideToClickedSlide: false,
		mousewheel: {
			forceToAxis: true,
			releaseOnEdges: true,
			thresholdDelta: 10,
		},
		navigation: {
			nextEl: node.querySelector('.js-next'),
			prevEl: node.querySelector('.js-prev'),
			disabledClass: 'disabled',
		},
	});

	swiper.on('slideChange', function () {
		window.za({ ev_action: 'click_arrows', ev_category: 'offer' });

		if (total <= offset) return;
		if (swiper.activeIndex < offset - limit/2) return;

		if (loading) return;
		loading = true;

		loadOffers().then(function(result){
			updateList(result.items);
			swiper.updateSlides();
			swiper.updateProgress();
			loading = false;
		});
	});

	requirejs(['jquery', 'js2/ZATimer'], function($, zaTimer) {
		var $sliderBlock = $(node).find('.js-slider-block').addBack('.js-slider-block');
		var $slides = $sliderBlock.find('.js-slider-item[data-za]');

		$slides.on('mousedown', '.js-org-url', function(e){
						var $slide = $(this).closest('li[data-za]');
			zaTimer.trigger($slide, {ev_action: 'display'});

			window.za($.extend({ev_action: 'open'}, $(this).data('za').data));
		});
	});
});
</script>

	</div>
	<script>
		(function () {
			const node = document.getElementById('offers');
			const slides = node.querySelectorAll('.js-slider-item');
			const orgId = '637cfb46c7adee4fd408ef6f.0311';

			// stat on container 0s visibility
			new VisibilityTracker(node, function () {
				window.za({
					ev_action: 'display_short',
					ev_category: 'offer',
					ev_sourceType: 'org_offer',
					ev_sourceId: orgId
				});
			}, { ratio: 0, duration: 0 }).track();

			// stat on container 3s visibility
			new VisibilityTracker(node, function () {
				window.za({ ev_action: 'display', ev_category: 'offer' });
			}, { ratio: 0.5 }).track();

			node.addEventListener('click', function (e) {
				const target = e.target.closest('.js-slider-item');
				if (!target) return;

				// stat on slide click
				window.za({
					ev_action: 'open',
					ev_category: 'offer',
					object_type: 'offer',
					object_id: target.dataset.offerId,
					ev_sourceType: 'org_offer',
					ev_sourceId: orgId
				});
			});

			slides.forEach(function (slide) {
				// stat on slide 0s visibility
				new VisibilityTracker(slide, function () {
					window.za({
						ev_action: 'display_short',
						ev_category: 'offer',
						object_type: 'offer',
						object_id: slide.dataset.offerId,
						ev_sourceType: 'org_offer',
						ev_sourceId: orgId
					});
				}, { ratio: 0, duration: 0 }).track();

				// stat on slide 3s visibility
				new VisibilityTracker(slide, function () {
					window.za({
						ev_action: 'display',
						ev_category: 'offer',
						object_type: 'offer',
						object_id: slide.dataset.offerId,
						ev_sourceType: 'org_offer',
						ev_sourceId: orgId
					});
				}, { ratio: 0 }).track();
			});
		})();
	</script>

