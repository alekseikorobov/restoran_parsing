<script>
	(function(){
		var define_rjs = window.define;

		var define = function () {
			if (typeof arguments[0] === 'string' && zrequire.has(arguments[0])) {
				zrequire.define.apply(this, arguments);
			}
			define_rjs.apply(this, arguments);
		};
		define.amd = true;

		window.define = define;
	})();
