<script>
zrequire(["build/shared/js/modal"], function(mc) {
	const node = document.getElementById('u2849815');
	const phoneBtn = node.querySelector('.js-org-control-phone');

	const modal = mc.create('phones', {
		parameters: { owner_id: '637cfb46c7adee4fd408ef6f', owner_type: 'organization' }
	});

	phoneBtn.addEventListener('click', function () {
		modal.open();
	})
});
</script>

			</div>
			</nav>
</div>
<script>
document.addEventListener('DOMContentLoaded', function() {
	const node = document.getElementById('serviceNavPanel');
	const nav = node.querySelector('.js-service-block-nav');
	const navWrapper = node.querySelector('.js-service-block-nav-wrapper');
	const navItems = Array.from(node.querySelectorAll('.js-nav-item'));

	const containers = navItems.map(function (item) {
		return document.getElementById(item.dataset.id);
	}).filter(Boolean);

	const navPanelHeight = node.offsetHeight;
	const positionTop = node.getBoundingClientRect().top + window.scrollY;
	let navPanelWidth = 0;
	const isLanding = Boolean('');

	const toggleNavFade = function () {
		if (navItems.length === 0) return;
		// if width doesn't change - skip
		if (navPanelWidth === nav.offsetWidth) return;

		navPanelWidth = nav.offsetWidth;
		const lastNavItem = navItems[navItems.length - 1];
		// box padding
		const GAP = 12;
		navWrapper.classList.toggle('_fade', lastNavItem.offsetLeft + lastNavItem.offsetWidth > navWrapper.offsetWidth + GAP);
	}

	const toggleNavFixed = function () {
		node.classList.toggle('_fixed', positionTop < window.scrollY);
	}

	const toggleNavActive = function () {
		if (isLanding) return;

		const visibleContainer = containers.find(function (container) {
			if (!container) return;

			const rect = container.getBoundingClientRect();
			return (rect.top - navPanelHeight) < 0 && (rect.bottom - navPanelHeight) > 0;
		});

		if (!visibleContainer) return;

		const activeNavItem = navItems.find(function (navItem) {
			return navItem.dataset.id === visibleContainer?.id;
		});

		navItems.forEach(function (navItem) {
			navItem.classList.toggle('service-block-nav-item__active', navItem === activeNavItem);
		});
	}

	const onScroll = function () {
		toggleNavFixed();
		toggleNavActive();
		toggleNavFade();
	}

	const scrollToContainer = function (hash) {
		if (!hash) return;

		const id = hash.replace('#', '');
		const container = document.getElementById(id);
		if (!container) return;

		let offset = navPanelHeight;
		if (id === 'info') offset = 0;

		window.scrollTo({
			top: container.getBoundingClientRect().top + window.scrollY - offset,
			behavior: 'smooth',
		});
	}

	node.addEventListener('click', function (e) {
		const navItem = e.target.closest('.js-nav-item');
		if (!navItem) return;

		const evLabel = navItem.dataset.contentName || navItem.dataset.id;
		window.za({ ev_category: 'nav_panel', ev_action: 'click', ev_label: evLabel });

		const navURL = new URL(navItem.href);
		const currentUrl = window.location;

		if (isLanding || (navURL.pathname !== currentUrl.pathname)) return;

		e.preventDefault();
		scrollToContainer(navURL.hash);
	});

	window.addEventListener('scroll', window.throttle(onScroll, 100), { passive: true });
	onScroll();

	window.addEventListener('resize', window.rafThrottle(toggleNavFade), { passive: true });
});
</script>



