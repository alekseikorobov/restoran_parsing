<script>
	(function () {
		const node = document.getElementById('u479423');
		const activator = node.querySelector('.js-share-activator');

		const shareData = { url: 'https://zoon.ru/volgograd/restaurants/kafe_epitsentr_vkusa_-_lavashok_na_angarskoj_ulitse/#comment658c9adfd8b478d57006857f' };

		const shareParams = {
			type: 'comment',
			objectId: '658c9adfd8b478d57006857f',
			organizationId: '637cfb46c7adee4fd408ef6f'
		};

		
		const dropdownNode = node.querySelector('.js-share-dropdown');
		const copyDefaultNode = node.querySelector('.js-share-copy');
		const copySuccessNode = node.querySelector('.js-share-copied');

		const dropdown = new Tooltip(dropdownNode, activator);

		dropdown.on('toggle', function (isVisible) {
			activator.classList.toggle('_active', isVisible);
		});

		function setShareUrl (url) {
			if (!url) return;

			shareData.url = url;

			const shareItems = dropdownNode.querySelectorAll('.js-share-item');
			shareItems.forEach(function (item) {
				if (item.classList.contains('js-share-copy')) {
					item.dataset.url = url;
					return;
				}

				item.href = item.dataset.url + encodeURIComponent(url);
			});
		}

		setShareUrl(shareData.url);

		activator.addEventListener('click', function (event) {
			event.stopPropagation();

			if (shareData.url) {
				dropdown.toggle();
				return;
			}

			zrequest.api('ShortLink.Get', shareParams).then(function (result) {
				if (!result.url) return;

				setShareUrl(result.url);
				dropdown.open();
			});
		});

		copyDefaultNode.addEventListener('click', function () {
			copyDefaultNode.classList.add('hidden');
			copySuccessNode.classList.remove('hidden');

			navigator.clipboard.writeText(copyDefaultNode.dataset.url);

			setTimeout(function () {
				copyDefaultNode.classList.remove('hidden');
				copySuccessNode.classList.add('hidden');
			}, 3000);
		});
		})()
</script>
</div>
