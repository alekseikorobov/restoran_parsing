<script>
	requirejs(['jquery', 'js2/RatingChangeBlock'], function($, RatingChangeBlock) {
		var $node = $('#u4548407').closest('.js-reviews-module');
		var itemId  = "637cfb46c7adee4fd408ef6f";
		var itemType = "organization";
		var userMark = 0;

		var comment_rating = new RatingChangeBlock($node.find('.feedbacks-form .js-user-rating-holder'), itemId, itemType, userMark, "user", "comment", true);
		comment_rating.init();
		comment_rating.forceUpdateRating(0);
		var $heading = $node.find('.js-heading-rating-holder');
		if ($heading.length) {
			var heading_rating = new RatingChangeBlock($heading, itemId, itemType, userMark, "user_total");
			heading_rating.init().on('ratingChange', function(mark, total, count){
				var $blocks = $node.find('.js-heading-rating-container');
				if ($blocks.length === 2) {
					$blocks.toggle();
				}
			});
		}

		var $comments_total_rating = $node.find('.js-comments-total-rating-holder');
		if ($comments_total_rating.length) {
			var total_rating = new RatingChangeBlock($comments_total_rating, itemId, itemType, userMark, "user_total");
			total_rating.init();
		}

		$(window).on('rating_change', function(e) {
			$node.find('.js-user-mark').html(e.user_rating).parent().show();
			$node.find('.js-total-mark').html(e.user_rating_stars);
			$node.find('.js-total-count').each(function() {
				var t = $(this).text();
				$(this).text(t.replace(/\d+/, e.total_count));
			});
		})

	});
</script>

