<script>
jqready(function($) {
	var reporter = function() {
		create_layer('report_error', { popstate: false });
		return false;
	};

	$('body').off('click.report_error', '.js-report-error').on('click.report_error', '.js-report-error', reporter);

	$(document).on('keydown', function(e) {
		if (e.ctrlKey && (e.keyCode == 13 || e.keyCode == 10)) {
			e.preventDefault();
			reporter();
		}
	});
});
</script>
