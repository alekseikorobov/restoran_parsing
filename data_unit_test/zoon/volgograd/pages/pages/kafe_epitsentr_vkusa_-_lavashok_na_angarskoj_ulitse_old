<script>
(function () {
	const node = document.getElementById('u1415224');
	const tooltipNode = node.querySelector('.js-mark-tooltip');
	const tooltipActivator = node.querySelector('.js-tooltip-activator');
	const tooltip = new Tooltip(tooltipNode, tooltipActivator);

	const userRatingMark = tooltipNode.querySelector('.js-user-rating-mark');
	const userRatingMarkValue = tooltipNode.querySelector('.js-user-rating-mark-value');
	const userRatingNoMark = tooltipNode.querySelector('.js-user-rating-no-mark');

	const stars = node.querySelectorAll('.star-item');

	tooltipActivator.addEventListener('mouseenter', function () {
		tooltip.open();
	});

	tooltipActivator.addEventListener('mouseleave', function () {
		tooltip.close();
	});

	stars.forEach(function (star) {
		star.addEventListener('mouseenter', function () {
			stars.forEach(function (currentStar) {
				currentStar.classList.toggle('star-selected', currentStar === star);
			});
		});
	});

	tooltipActivator.addEventListener('mouseleave', function () {
		const myMark = tooltipActivator.dataset.my;

		stars.forEach(function (star, index) {
			star.classList.toggle('star-selected', (myMark !== '') && (Number(myMark) === index + 1));
		});
	});

	window.addEventListener('user_rating_change', function (event) {
		userRatingNoMark.classList.add('hidden');
		userRatingMark.classList.remove('hidden');
		userRatingMarkValue.textContent = event.detail.user_rating;
	});
})()
</script>

				</div>
							</div>

			
